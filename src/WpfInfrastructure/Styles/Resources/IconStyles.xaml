    <Style x:Key="Icon.SoonFilled"
           TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Viewbox Height="{TemplateBinding Height}"
                             Width="{TemplateBinding Width}">
                        <Grid Background="{TemplateBinding Background}" 
                              Height="200" Width="200">
                            <ContentControl Foreground="{TemplateBinding Foreground}"
                                            Height="60"
                                            Margin="-120,-120,0,0"
                                            Style="{StaticResource Icon.WarnSign}" 
                                            Width="60"/>
                            <Path Data="{StaticResource StreamGeometry.SoonFilledSymbol}"
                                  Fill="{TemplateBinding Foreground}"
                                  Margin="70,40,0,0"/>
                            <TextBlock Margin="120, 135,0,0" FontSize="26" FontWeight="Bold"  Text="{TemplateBinding Content}"/>
