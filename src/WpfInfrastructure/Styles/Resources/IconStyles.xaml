    <Style x:Key="Icon.PrintModule"
              TargetType="{x:Type ContentControl}">
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContentControl}">
                    <Border Background="{TemplateBinding Foreground}">
						<Viewbox Height="Auto"
								 Width="Auto" Margin="5">
							<Grid Background="{TemplateBinding Background}" 
								  Height="200" Width="200">
								<Path Data="{StaticResource StreamGeometry.PrintModuleSymbol}"
									  Fill="{TemplateBinding Foreground}"
									  Margin="33.334,45.833,33.333,56.667" Stretch="Fill"/>
								<TextBlock Margin="92.412,88.306,92.631,75.444" FontSize="26" FontWeight="Bold"  Text="{TemplateBinding Content}" HorizontalAlignment="Stretch" VerticalAlignment="Center" Height="36.25"/>
							</Grid>
						</Viewbox>
					</Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

